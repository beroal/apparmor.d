abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/xeyes
profile xeyes /{,usr/}{,s}bin/xeyes {
  include <abstractions/base>
  include <abstractions/X-strict>

  @{exec_path} mr,

  include if exists <local/xeyes>
}

# vim:syntax=apparmor
