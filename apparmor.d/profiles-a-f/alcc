abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/alcc
profile alcc @{exec_path} {
  include <abstractions/base>
  include <abstractions/consoles>
  include <abstractions/user-download-strict>

  @{exec_path} mr,

  include if exists <local/alcc>
}

# vim:syntax=apparmor
